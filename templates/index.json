    "custom_liquid_p3PFDj": {
      "type": "custom-liquid",
      "disabled": true,
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "display_id": false,
        "visibility": "always-display",
        "custom_liquid": "<!-- üéÆ GAME-BRO - THE LATE HALLOWEEN POPUP -->\n<div id=\"halloween-popup\" \n     style=\"display:none; position:fixed; top:0; left:0; width:100%; height:100%;\n            background:rgba(0,0,0,0.85); z-index:9999; justify-content:center; align-items:center;\n            transition:opacity 0.4s ease;\">\n\n  <!-- Conteneur -->\n  <div style=\"position:relative; background:#000; border-radius:14px; overflow:hidden;\n              max-width:430px; width:90%; text-align:center; \n              box-shadow:0 0 30px rgba(114,9,183,0.7); animation:popIn 0.4s ease;\">\n    \n    <!-- Image principale -->\n    <img src=\"https://cdn.shopify.com/s/files/1/0951/3700/5951/files/For_the_Halloweeners.png?v=1762093215\" \n         alt=\"For the Halloweeners\" \n         style=\"width:100%; display:block;\">\n\n    <!-- Texte + D√©compteur -->\n    <div style=\"padding:16px 14px 6px; color:#fff; font-family:Poppins, sans-serif;\">\n      <h3 style=\"margin:0; font-size:15px; font-weight:700; letter-spacing:0.4px; color:#FFB703;\">\n      <p style=\"margin:8px 0 0; font-size:13px; opacity:0.9;\">\n        Ends in <span id=\"popup-timer\" style=\"color:#FFB703; font-weight:700;\">00d 00h 00m 00s</span>\n      </p>\n    </div>\n\n    <!-- Bouton CTA -->\n    <a href=\"https://game-bro.com/products/game-bro-console\" target=\"_blank\"\n       style=\"display:inline-block; background:linear-gradient(90deg, #3A0CA3, #7209B7);\n              color:#fff; font-weight:700; text-decoration:none; margin:12px 0 18px; \n              padding:10px 26px; border-radius:8px; font-size:13.5px; \n              transition:transform 0.2s ease, background 0.3s ease;\">\n      Shop Now\n    </a>\n\n    <!-- Bouton fermer -->\n    <button onclick=\"closeHalloweenPopup()\" \n            style=\"position:absolute; top:8px; right:10px; background:none; border:none; \n                   color:#fff; font-size:20px; font-weight:700; cursor:pointer;\">‚úï</button>\n  </div>\n</div>\n\n<!-- Animation -->\n<style>\n@keyframes popIn {\n  from { transform:scale(0.9); opacity:0; }\n  to { transform:scale(1); opacity:1; }\n}\n</style>\n\n<!-- Script -->\n<script>\n  // Compteur jusqu‚Äôau 4 novembre 23:59\n  function updatePopupTimer() {\n    const endDate = new Date(\"2025-11-06T23:59:59\").getTime();\n    const now = new Date().getTime();\n    const distance = endDate - now;\n\n    if (distance <= 0) {\n      document.getElementById(\"popup-timer\").innerHTML = \"EXPIRED\";\n      return;\n    }\n\n    const days = Math.floor(distance / (1000 * 60 * 60 * 24));\n    const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\n    const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));\n    const seconds = Math.floor((distance % (1000 * 60)) / 1000);\n\n    document.getElementById(\"popup-timer\").innerHTML =\n      `${days}d ${hours}h ${minutes}m ${seconds}s`;\n  }\n\n  // Lancement du compteur\n  updatePopupTimer();\n  setInterval(updatePopupTimer, 1000);\n\n  // Affichage auto du pop-up apr√®s 4 secondes\n  window.addEventListener(\"load\", function() {\n    setTimeout(() => {\n      document.getElementById(\"halloween-popup\").style.display = \"flex\";\n    }, 4000);\n  });\n\n  // Fermeture du pop-up\n  function closeHalloweenPopup() {\n    const popup = document.getElementById(\"halloween-popup\");\n    popup.style.opacity = \"0\";\n    setTimeout(() => popup.style.display = \"none\", 400);\n  }\n\n  // Fermer si clic ext√©rieur\n  document.addEventListener(\"click\", function(e) {\n    const popup = document.getElementById(\"halloween-popup\");\n    if (popup.style.display === \"flex\" && !e.target.closest(\"#halloween-popup div\")) {\n      closeHalloweenPopup();\n    }\n  });\n</script>",
        "color_scheme": "background-1",
        "padding_top": 0,
        "padding_bottom": 16
      }
    },
