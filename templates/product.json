        "custom_liquid_dXzmDW": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.available == false %}\n<div class=\"restock-container\" style=\"text-align:center; background:#0a0a0a; padding:25px 18px; border-radius:16px; margin-top:30px; box-shadow:0 0 20px rgba(120,0,255,0.15); color:#fff;\">\n  \n  <h2 style=\"font-size:24px; font-weight:700; color:#fff; margin-bottom:8px;\">\n    Be the first to know when {{ product.title }} is back!\n  </h2>\n  <p style=\"font-size:15px; color:#bbb; margin-bottom:20px; line-height:1.4;\">\n    Select your favorite color and enter your email below — we’ll notify you as soon as it’s restocked.\n  </p>\n\n  <form id=\"restock-form\" style=\"max-width:380px; margin:0 auto;\">\n    \n    <!-- Select Color -->\n    <label for=\"restock-color\" style=\"display:block; text-align:left; font-weight:600; margin-bottom:4px; font-size:14px;\">Color</label>\n    <select id=\"restock-color\" name=\"color\" required\n      style=\"width:100%; padding:10px; border-radius:8px; border:1px solid #444; background:#111; color:#fff; margin-bottom:12px; font-size:14px;\">\n      <option value=\"\">Select color</option>\n      <option>Blue Quest</option>\n      <option>Electric Yellow</option>\n      <option>Mystic Flame</option>\n      <option>Emerald Mode</option>\n      <option>Crimson Creed</option>\n      <option>Nitro Blaze</option>\n      <option>Dark Rose</option>\n      <option>White Strike</option>\n      <option>Midnight Dash</option>\n    </select>\n\n    <!-- Email Field -->\n    <input type=\"email\" id=\"restock-email\" name=\"email\" placeholder=\"Enter your email address\" required\n      style=\"width:100%; padding:10px; border:1px solid #444; border-radius:8px; background:#111; color:#fff; margin-bottom:12px; font-size:14px;\">\n    \n    <!-- Submit Button -->\n    <button type=\"submit\"\n      style=\"width:100%; padding:12px; background:linear-gradient(90deg, #7b2ff7, #f107a3); color:#fff; font-size:15px; border:none; border-radius:8px; font-weight:600; cursor:pointer; transition:opacity .3s;\">\n      Notify Me When Available\n    </button>\n  </form>\n\n  <p id=\"restock-message\" style=\"display:none; margin-top:10px; color:#00ffb3; font-size:14px;\">\n    ✅ Thanks! You’ll receive an email when your selected color is back in stock.\n  </p>\n</div>\n\n<script>\ndocument.getElementById('restock-form').addEventListener('submit', function(e) {\n  e.preventDefault();\n  \n  const email = document.getElementById('restock-email').value;\n  const color = document.getElementById('restock-color').value;\n  \n  if(!color) {\n    alert('Please select a color first!');\n    return;\n  }\n\n  fetch('/contact', {\n    method: 'POST',\n    headers: {'Content-Type': 'application/x-www-form-urlencoded'},\n    body: new URLSearchParams({\n      'form_type': 'contact',\n      'utf8': '✓',\n      'contact[email]': email,\n      'contact[tags]': 'restock-notification {{ product.title }} - ' + color\n    })\n  }).then(() => {\n    document.getElementById('restock-message').style.display = 'block';\n    document.getElementById('restock-form').reset();\n  });\n});\n</script>\n{% endif %}"
          }
        },
